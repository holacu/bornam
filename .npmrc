# إعدادات npm للنشر على Railway
optional=false
audit=false
fund=false
progress=false
loglevel=warn

# تجاهل أخطاء التبعيات الاختيارية
ignore-scripts=true
unsafe-perm=true

# منع تثبيت raknet-native
package-lock=false
legacy-peer-deps=true

# إعدادات البناء
rebuild-bundle=false
build-from-source=false

# تحسين الأداء
cache-max=86400000
prefer-offline=false
